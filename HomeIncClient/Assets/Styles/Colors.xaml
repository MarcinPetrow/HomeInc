<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="MainBorderColor" Color="#FF506263"></SolidColorBrush>
    <SolidColorBrush x:Key="HeaderBackgroundColor" Color="#FF235770"></SolidColorBrush>
    <SolidColorBrush x:Key="BackgroundColor" Color="White"></SolidColorBrush>

    <SolidColorBrush x:Key="ControlBackgroundColor" Color="#FF96C9CF"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlHighlightedBackgroundColor" Color="#FFC6E4E8"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlDisabledBackgroundColor" Color="#FFCBCBCB"></SolidColorBrush>

    <SolidColorBrush x:Key="ControlBorderColor" Color="#FF59AEB8"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlHighlightedBorderColor" Color="#FF86CBD3"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlDisabledBorderColor" Color="#FFBBBBBB"></SolidColorBrush>

    <SolidColorBrush x:Key="ControlForegroundColor" Color="#FF323232"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlHighlightedForegroundColor" Color="#FF505050"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlDisabledForegroundColor" Color="#FF979797"></SolidColorBrush>

</ResourceDictionary>