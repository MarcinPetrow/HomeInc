<UserControl x:Class="HomeIncClient.Views.Partials.QuickAccessBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Orientation="Horizontal">
        <Button Command="{Binding RouteRootCommand}">Home</Button>
        <Button Margin="5,0,0,0" Command="{Binding RouteRootCommand}">Transactions</Button>
        <Button Margin="5,0,0,0" Command="{Binding RouteRootCommand}">Categories</Button>
        <Button Margin="5,0,0,0" Command="{Binding RouteRootCommand}">Statistics</Button>
        <Button Margin="5,0,0,0" Command="{Binding RouteRootCommand}">Settings</Button>
        <Button Margin="5,0,0,0" Command="{Binding RouteRootCommand}">About</Button>
    </StackPanel>
</UserControl>