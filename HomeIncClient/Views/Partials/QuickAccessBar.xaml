<UserControl x:Class="HomeIncClient.Views.Partials.QuickAccessBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             >
    <StackPanel Orientation="Horizontal">
        <Button Command="{Binding RouteRootCommand}">Home</Button>
        <Button Command="{Binding RouteRootCommand}" Margin="5,0,0,0">Transactions</Button>
        <Button Command="{Binding RouteRootCommand}" Margin="5,0,0,0">Categories</Button>
        <Button Command="{Binding RouteRootCommand}" Margin="5,0,0,0">Statistics</Button>
        <Button Command="{Binding RouteRootCommand}" Margin="5,0,0,0">Settings</Button>
        <Button Command="{Binding RouteRootCommand}" Margin="5,0,0,0">About</Button>
    </StackPanel>
</UserControl>
